<h1>Spying on <%= @user.username %></h1>
<ul>
  <% @known_ips.each do |ip, users| %>
    <li><h4><%= link_to ip, mod_audit_index_path(ip: @user.ip_address) %></h4>
      <% if users.any? %>
        <ul>
          <% users.each do |user| %>
            <li><%= link_to "#{user.id} #{user.username}", user %></li>
          <% end %>
        </ul>
      <% end %>
    </li>
  <% end %>
</ul>
