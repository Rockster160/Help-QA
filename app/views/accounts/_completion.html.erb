<ul class="account-completion">
  <% completion.each do |achievement, completed| %>
    <li class="achievement-<%= completed ? 'complete' : 'incomplete' %>">
      <% if completed %>
        <%= hover_icon("check", "complete") %>
      <% else %>
        <%= hover_icon("cross", "incomplete") %>
      <% end %>
      <%= achievement %>
    </li>
  <% end %>
</ul>
